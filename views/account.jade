doctype html
html(ng-app="biApp")
    head
        meta(charset='utf8')
        base(href='/')
        title Course app demo
        link(rel='stylesheet', href='/css/app.css')
        link(href='http://fonts.googleapis.com/css?family=Oswald', rel='stylesheet' ,type='text/css')
        link(href='http://fonts.googleapis.com/css?family=Roboto:400,700', rel='stylesheet', type='text/css')
        script(src='http://cdnjs.cloudflare.com/ajax/libs/jquery/1.7.1/jquery.min.js')
    body(ng-controller='AccountCtrl')
        include partials/account/account-header
        div.row
            aside.col-md-2.account-aside
                include partials/account/account-aside-nav

                //Modals
                include partials/account/account-addstudent-modal
                include partials/account/account-addcourse-modal
            div.col-md-8
                include partials/account/account-notyfications
                include partials/account/account-list-students

    footer#footer
        script(src='js/lib/jQuery.js')
        script(src='js/lib/bootstrap.min.js')
        script(src='js/lib/parallaxScroll.js')
        script(src='js/lib/scripts.js')


        script(src='js/lib/angular/angular.js')
        script(src='http://angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.9.0.js')
        script(src='http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.5.2/underscore-min.js')
        script(src='http://cdn.jsdelivr.net/restangular/1.1.3/restangular.min.js ')
        //script(src='/socket.io/socket.io.js')
        script(src='js/app.js')
        script(src='js/directives/DomModule.js')
        script(src='js/directives/TabDirective.js')
        script(src='js/services/AccountApi.js')
        script(src='js/services/SocketService.js')
        script(src='js/controllers/AppCtrl.js')
        script(src='js/controllers/AccountCtrl.js')